<div class="container">
  <div class="row mt-5">
     <h4 class="ml-5 bg-light">ジャンル編集</h4>

  </div>
  <%= render "public/shared/error_messages", resource: @genre %>
  <div class="row mt-5">
    <%= form_with model: @genre, url: admin_genre_path, method: :patch, local: true do |f| %>
      <%= f.label :ジャンル名 %>
      <%= f.text_field :name %>
      <%= f.submit "変更を保存", class:"btn btn-success" %>
    <% end %>
  </div>
</div>